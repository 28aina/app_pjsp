[n_typedocument] iNTEGER  NULL PRIMARY KEY,
[activite] teXT  NULL
);
CREATE TABLE [typepiece] (
[n_typepiece] inTEGER  PRIMARY KEY NULL,
[type_piece] teXT  NULL
);
